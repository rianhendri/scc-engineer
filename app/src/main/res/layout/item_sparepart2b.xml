<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:orientation="vertical"
	>
	<LinearLayout
		android:visibility="gone"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal">
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:layout_weight="1"
			>
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal">
				<TextView
					android:id="@+id/nospart"
					android:layout_width="20dp"
					android:layout_height="wrap_content"
					android:layout_marginLeft="8dp"
					android:layout_marginTop="8dp"
					android:layout_marginBottom="8dp"
					android:fontFamily="@font/segoeui"
					android:textSize="11dp"
					android:text="10"/>
				<TextView
					android:id="@+id/codecd"
					android:layout_width="70dp"
					android:layout_height="wrap_content"
					android:layout_marginLeft="8dp"
					android:layout_marginRight="8dp"
					android:layout_marginTop="8dp"
					android:layout_marginBottom="8dp"
					android:fontFamily="@font/segoeui"
					android:textSize="11dp"
					android:text="CT354-01350"/>
				<TextView
					android:id="@+id/namesparq"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="8dp"
					android:layout_marginRight="8dp"
					android:layout_marginTop="8dp"
					android:layout_marginBottom="8dp"
					android:fontFamily="@font/segoeui"
					android:textSize="11dp"
					android:text="[CT354-01360] - FAN ASSY. WIRED- INK UNIT"/>
			</LinearLayout>

			<View
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/softgrey"
				android:layout_marginLeft="8dp"
				android:layout_marginRight="8dp"
				/>
		</LinearLayout>
		<ImageView
			android:id="@+id/deletelista"
			android:layout_width="35dp"
			android:layout_height="match_parent"
			android:layout_weight="1"

			android:src="@drawable/ic_trash"
			android:textSize="12sp" />
	</LinearLayout>
	<LinearLayout
		android:id="@+id/layoutsperpart"
		android:visibility="visible"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		android:paddingBottom="8dp"
		android:paddingTop="8dp"
		android:gravity="center_vertical"
		android:background="@color/dwhite"
		>
		<LinearLayout
			android:layout_width="100dp"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:layout_weight="1"
			android:layout_marginRight="4dp"
			android:layout_marginLeft="8dp"
			android:layout_marginTop="2dp"
			android:layout_marginBottom="2dp">
			<TextView
				android:id="@+id/namespar"
				android:layout_width="100dp"
				android:layout_height="wrap_content"

				android:fontFamily="@font/segoeui"
				android:textSize="11dp"
				android:layout_weight="1"
				android:maxLines="10"
				android:text=" "
				android:textColor="@color/black"/>
			<TextView
				android:id="@+id/statussper"
				android:layout_width="90dp"
				android:layout_height="wrap_content"
				android:layout_marginLeft="0dp"
				android:layout_marginRight="8dp"
				android:layout_marginTop="2dp"
				android:layout_marginBottom="2dp"
				android:fontFamily="@font/segoeui"
				android:textSize="11dp"
				android:text="-"
				android:maxWidth="120dp"
				android:maxLength="100"
				android:enabled="false"
				android:layout_weight="1"
				android:background="@drawable/underline"
				android:textColor="@color/black"/>
		</LinearLayout>
		<EditText
			android:id="@+id/pricea"
			android:layout_width="70dp"
			android:inputType="number"
			android:enabled="false"
			android:layout_height="wrap_content"
			android:layout_marginLeft="4dp"
			android:layout_marginRight="8dp"
			android:layout_marginTop="2dp"
			android:layout_marginBottom="2dp"
			android:fontFamily="@font/segoeui"
			android:textSize="11dp"
			android:gravity="start"
			android:text=" "
			android:background="@color/transparent"
			android:textColor="@color/black"/>
		<TextView
			android:id="@+id/qtysper"
			android:layout_width="30dp"
			android:inputType="number"
			android:enabled="false"
			android:layout_height="wrap_content"
			android:layout_marginLeft="4dp"
			android:layout_marginRight="8dp"
			android:layout_marginTop="2dp"
			android:layout_marginBottom="2dp"
			android:fontFamily="@font/segoeui"
			android:textSize="11dp"
			android:gravity="center"
			android:text=" "
			android:background="@drawable/underline"
			android:textColor="@color/black"/>
		<EditText
			android:id="@+id/totalpricea"
			android:layout_width="70dp"
			android:inputType="number"
			android:enabled="false"
			android:layout_height="wrap_content"
			android:layout_marginLeft="4dp"
			android:layout_marginRight="8dp"
			android:layout_marginTop="2dp"
			android:layout_marginBottom="2dp"
			android:fontFamily="@font/segoeui"
			android:textSize="11dp"
			android:gravity="start"
			android:text=" "
			android:background="@color/transparent"
			android:textColor="@color/black"/>
		<TextView
			android:id="@+id/reason"
			android:layout_width="100dp"
			android:layout_height="wrap_content"
			android:layout_marginLeft="4dp"
			android:layout_marginRight="8dp"
			android:maxWidth="100dp"
			android:enabled="false"
			android:layout_marginTop="2dp"
			android:layout_marginBottom="2dp"
			android:fontFamily="@font/segoeui"
			android:textSize="11dp"
			android:background="@drawable/underline"
			android:text=" "
			android:layout_weight="1"
			android:maxLength="100"
			android:textColor="@color/black"/>
		<TextView
			android:id="@+id/caseid"
			android:layout_width="100dp"
			android:layout_height="wrap_content"
			android:layout_marginLeft="4dp"
			android:layout_marginRight="8dp"
			android:layout_marginTop="2dp"
			android:layout_marginBottom="2dp"
			android:enabled="false"
			android:fontFamily="@font/segoeui"
			android:textSize="11dp"
			android:text=" "
			android:maxWidth="100dp"
			android:maxLength="100"
			android:layout_weight="1"
			android:background="@drawable/underline"
			android:textColor="@color/black"/>
		<TextView
			android:id="@+id/orderdate"
			android:layout_width="120dp"
			android:layout_height="wrap_content"
			android:layout_marginLeft="4dp"
			android:layout_marginRight="8dp"
			android:layout_marginTop="2dp"
			android:layout_marginBottom="2dp"
			android:fontFamily="@font/segoeui"
			android:textSize="11dp"
			android:text="-"
			android:maxWidth="120dp"
			android:maxLength="100"
			android:layout_weight="1"
			android:textColor="@color/black"/>
		<TextView
		android:id="@+id/installdate"
		android:layout_width="120dp"
		android:layout_height="wrap_content"
		android:layout_marginLeft="4dp"
		android:layout_marginRight="8dp"
		android:layout_marginTop="2dp"
		android:layout_marginBottom="2dp"
		android:fontFamily="@font/segoeui"
		android:textSize="11dp"
		android:text="-"
		android:maxWidth="120dp"
		android:maxLength="100"
		android:enabled="false"
		android:layout_weight="1"
		android:background="@drawable/underline"
		android:textColor="@color/black"/>
		<TextView
			android:id="@+id/stock"
			android:layout_width="90dp"
			android:layout_height="wrap_content"
			android:layout_marginLeft="4dp"
			android:layout_marginRight="8dp"
			android:layout_marginTop="2dp"
			android:layout_marginBottom="2dp"
			android:fontFamily="@font/segoeui"
			android:textSize="11dp"
			android:text="-"
			android:maxWidth="120dp"
			android:maxLength="100"
			android:enabled="false"
			android:layout_weight="1"
			android:background="@drawable/underline"
			android:textColor="@color/black"/>
		<ImageView
		android:id="@+id/deletelist"
		android:layout_width="30dp"
		android:layout_height="15dp"


		android:src="@drawable/ic_trash"
		/>
		<ImageView
			android:id="@+id/deletelist2"
			android:layout_width="30dp"
			android:layout_height="15dp"

			/>
	</LinearLayout>

</LinearLayout>
